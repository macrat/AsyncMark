{
  "name": "asyncmark",
  "version": "0.3.6",
  "description": "A benchmarking library that supports Promise.",
  "main": "dist/asyncmark",
  "types": "index.d.ts",
  "unpkg": "dist/asyncmark.min.js",
  "files": [
    "README.md",
    "LICESNSE",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint --ext .ts,.js --ignore-pattern '/examples' --ignore-pattern '/on-web' .",
    "build": "npm-run-all build:*",
    "build:dist": "cross-env NODE_ENV=production node build.js",
    "build:docs": "cross-env NODE_ENV=production esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macrat/AsyncMark.git"
  },
  "keywords": [
    "benchmark",
    "benchmarking",
    "promise",
    "asynchronous",
    "async"
  ],
  "author": "MacRat <m@crat.jp> (https://blanktar.jp)",
  "license": "MIT",
  "bugs": "https://github.com/macrat/AsyncMark/issues",
  "homepage": "https://macrat.github.io/AsyncMark/",
  "devDependencies": {
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/jest": "^26.0.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "cross-env": "^7.0.2",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rollup": "^2.18.2",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^26.1.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
  "runkitExample": "const AsyncMark = require('asyncmark');\n\nnew AsyncMark.Benchmark(function() {\n    return new Promise((resolve, reject) => {\n        setTimeout(resolve, 100);\n    });\n}).run().catch(console.error);\n",
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "test": {
            "source": "./test/",
            "includes": [
              ".*\\.js$"
            ]
          }
        }
      },
      {
        "name": "esdoc-importpath-plugin",
        "option": {
          "replaces": [
            {
              "from": "^.*$",
              "to": "dist/asyncmark.mjs"
            }
          ]
        }
      }
    ]
  }
}
