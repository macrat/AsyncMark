{
  "name": "promise-bench",
  "version": "0.0.0",
  "description": "A benchmarking library that supports Promise.",
  "main": "dist/promise-bench.js",
  "files": [
    "README.md",
    "LICESNSE",
    "promise-bench.js"
  ],
  "dependencies": {
    "performance-now": "^2.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:dist && npm run build:docs",
    "build:dist": "mkdir dist; ./node_modules/.bin/babel src/index.js -d dist && ln -sf ../src/index.js dist/promise-bench.mjs",
    "build:docs": "./node_modules/.bin/esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macrat/PromiseBench.git"
  },
  "keywords": [
    "benchmark",
    "benchmarking",
    "promise",
    "asynchronous",
    "async"
  ],
  "author": "MacRat <m@crat.jp> (https://blanktar.jp)",
  "license": "MIT",
  "bugs": "https://github.com/macrat/PromiseBench/issues",
  "homepage": "https://macrat.github.io/PromiseBench/",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0"
  },
  "babel": {"presets": ["es2015"]},
  "esdoc": {
    "source": "./dist",
    "includes": [
      "promise-bench\\.mjs"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  }
}
