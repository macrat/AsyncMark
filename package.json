{
  "name": "promise-bench",
  "version": "0.0.0",
  "description": "A benchmarking library that supports Promise.",
  "main": "dist/promise-bench.js",
  "files": [
    "README.md",
    "LICESNSE",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:dist && npm run build:docs",
    "build:dist": "node build.js",
    "build:docs": "./node_modules/.bin/esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macrat/PromiseBench.git"
  },
  "keywords": [
    "benchmark",
    "benchmarking",
    "promise",
    "asynchronous",
    "async"
  ],
  "author": "MacRat <m@crat.jp> (https://blanktar.jp)",
  "license": "MIT",
  "bugs": "https://github.com/macrat/PromiseBench/issues",
  "homepage": "https://macrat.github.io/PromiseBench/",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "rollup": "^0.55.5",
    "rollup-plugin-babel": "^3.0.3"
  },
  "esdoc": {
    "source": "./dist",
    "includes": [
      "promise-bench\\.mjs"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  }
}
